name: list the artifacts in the repo

on:
  push:
    branches:
      - sumanshekhar23-patch-1

jobs:

  apicall:
    runs-on: ubuntu-latest
    steps:
    - name: hit api
      run: |
        curl -L \
        -H "Accept: application/vnd.github+json" \
        -H "Authorization: Bearer <YOUR-TOKEN>" \
        -H "X-GitHub-Api-Version: 2022-11-28" \
         https://api.github.com/repos/sumanshekhar23/jenkinspipeline/actions/artifacts

    - name: echo run
      run: echo "run ho gaya"
